{
  "expo": {
    "name": "Safha",
    "slug": "safha",
    "version": "1.0.0",
    "description": "Safha organizes all your interests from trusted sources. More than news - tech, sports, lifestyle, and everything you care about, summarized by AI.",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "safha",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.safha.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This allows us to personalize your news experience based on your reading preferences.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:safha.app"
      ]
    },
    "android": {
      "package": "com.safha.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "safha.app",
              "pathPrefix": "/subscription"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "isProduction": false,
      "router": {},
      "eas": {
        "projectId": "bc82dd94-5be7-4f5c-85b8-54ebebc75cf9"
      }
    },
    "owner": "mosaad515"
  }
}
